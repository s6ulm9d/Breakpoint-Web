{
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/v1/(.*)",
            "destination": "/api/v1/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}